<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.zhy.Mapping.mapper.userMapper">
   <select id="getUser" parameterType="int" 
        resultType="com.zhy.Model.User.User">
         select * from person
    </select>
    <select id="setName" parameterType="int" resultType ="com.zhy.Model.User.Name">
    	select name from person where age=12
    </select>
 </mapper>